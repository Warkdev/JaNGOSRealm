<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/jangoscharacters</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.show_sql">false</property>
    <property name="hibernate.query.factory_class">org.hibernate.hql.internal.classic.ClassicQueryTranslatorFactory</property>
    <mapping class="eu.jangos.realm.model.characters.Guild"/>
    <mapping class="eu.jangos.realm.model.characters.Characters"/>
    <mapping class="eu.jangos.realm.model.characters.Ticket"/>
    <mapping class="eu.jangos.realm.model.characters.GuildRankId"/>
    <mapping class="eu.jangos.realm.model.characters.Mail"/>
    <mapping class="eu.jangos.realm.model.characters.Social"/>
    <mapping class="eu.jangos.realm.model.characters.ItemInstance"/>
    <mapping class="eu.jangos.realm.model.characters.Reputation"/>
    <mapping class="eu.jangos.realm.model.characters.ReputationId"/>
    <mapping class="eu.jangos.realm.model.characters.GuildEventsId"/>
    <mapping class="eu.jangos.realm.model.characters.Skills"/>
    <mapping class="eu.jangos.realm.model.characters.SkillsId"/>
    <mapping class="eu.jangos.realm.model.characters.Spell"/>
    <mapping class="eu.jangos.realm.model.characters.GuildEventType"/>
    <mapping class="eu.jangos.realm.model.characters.GuildEvents"/>
    <mapping class="eu.jangos.realm.model.characters.ActionbarId"/>
    <mapping class="eu.jangos.realm.model.characters.Whispers"/>
    <mapping class="eu.jangos.realm.model.characters.SpellId"/>
    <mapping class="eu.jangos.realm.model.characters.Actionbar"/>
    <mapping class="eu.jangos.realm.model.characters.GuildRank"/>
    <mapping class="eu.jangos.realm.model.characters.ItemStorageType"/>
    <mapping class="eu.jangos.realm.model.characters.SocialId"/>
  </session-factory>
</hibernate-configuration>
