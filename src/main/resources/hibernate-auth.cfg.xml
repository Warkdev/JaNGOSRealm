<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/jangosauth</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.query.factory_class">org.hibernate.hql.internal.classic.ClassicQueryTranslatorFactory</property>
    <property name="hibernate.show_sql">false</property>
    <mapping resource="eu/jangos/realm/model/auth/Parameter1.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Account.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Realmtype.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Locale.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Realm.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Roles.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Commands.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/RealmAccount.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Realmtimezone.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Bannedaccount.hbm.xml"/>
    <mapping resource="eu/jangos/realm/model/auth/Bannedip.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
